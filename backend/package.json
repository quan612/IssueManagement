{
  "name": "backend",
  "scripts": {
    "start": "node -e graphql node -r dotenv/config src/index.js",
    "start:dev": "nodemon -e js,graphql -x node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "nodemon": "nodemon",
    "deploy": "prisma deploy --env-file variables.env"
  },
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.4",
    "graphql-cli": "^3.0.14",
    "graphql-yoga": "1.18.3",
    "jsonwebtoken": "8.5.1",
    "prisma-binding": "^2.3.16",
    "prisma-client-lib": "1.34.10"
  },
  "devDependencies": {
    "dotenv": "^6.2.0",
    "nodemon": "1.19.4",
    "prisma": "1.34.10"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
